    {{#if usernameMaroto}}
    <ul>
      {{#each username}}
        <li>Nome: {{fullName}}</li>
      {{/each}}
    </ul>
    {{/if}}

    {{#> footer}}
      {{!-- Hero content goes here. --}}
    {{/footer}}
  </body>
</html>

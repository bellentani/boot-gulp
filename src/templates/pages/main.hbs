<!DOCTYPE html>
  <head>
    <title>Front-End Dev Conference</title>
    <link rel="stylesheet" type="text/css" href="css/test.css">
  </head>
  <body>
    {{> header}}
    <main class="main">
      <h1>{{username.0.fullName}}</h1>
      <p>{{username.0.bio}}</p>
      <p>
        You can find me on <a href="http://www.twitter.com/{{username.0.twitterUsername}}">Twitter</a>,
        <a href="http://www.github.com/{{username.0.githubUsername}}">GitHub</a> or my <a href="http://{{username.1.website}}">Website</a>.
      </p>

      {{> jumbotron world='Marte'}}

      {{> user-loop }}
    </main>
    {{> footer}}

    <!--build:js js/main.js -->
    <script src="js/test.js"></script>
    <script src="js/outro.js"></script>
    <!-- endbuild -->

  </body>
</html>
